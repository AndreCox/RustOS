{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Kernel Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/target/x86_64-kernel/debug/RustOS",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "rust-gdb",
      "miDebuggerServerAddress": "localhost:1234",
      "launchCompleteCommand": "None",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set architecture to x86_64",
          "text": "set arch i386:x86-64",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "make-iso-debug"
    }
  ]
}
